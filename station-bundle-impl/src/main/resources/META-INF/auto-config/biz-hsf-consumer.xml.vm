<?xml version="1.0" encoding="GB2312"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-beans-3.0.xsd"
	default-autowire="byName">

	
	<bean id="taobaoTradeOrderQueryService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean">
        <property name="interfaceName">
            <value>com.taobao.cun.service.trade.TaobaoTradeOrderQueryService</value>
        </property>
        <property name="version">
            <value>${cuntao.taobaoTradeOrderQueryService.version}</value>
        </property>
    </bean> 
    
	
	<bean id="buyerQueryService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
        <property name="interfaceName">
            <value>com.taobao.trade.platform.api.query.BuyerQueryService</value>
        </property>
        <property name="version">
            <value>${cuntao.center.taobao.buyerQueryService.version}</value>
        </property>
    </bean>
    
    <bean id="refundReadService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
        <property name="interfaceName">
            <value>com.taobao.refundplatform.client.read.RefundReadService</value>
        </property>
        <property name="version">
            <value>${cuntao.center.taobao.refundReadService.version}</value>
        </property>
    </bean>
</beans>
