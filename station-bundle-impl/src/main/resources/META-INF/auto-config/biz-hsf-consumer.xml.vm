<?xml version="1.0" encoding="GB2312"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-beans-3.0.xsd"
	default-autowire="byName">

	<bean id="trainingRecordServiceFacade" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="syncInit">
		<property name="interfaceName" value="com.alibaba.ivy.service.user.TrainingRecordServiceFacade" />
		<property name="version" value="${partner.peixun.service.version}" />
	</bean>
	
	<bean id="courseServiceFacade" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="syncInit">
		<property name="interfaceName" value="com.alibaba.ivy.service.course.CourseServiceFacade" />
		<property name="version" value="${partner.peixun.service.version}" />
	</bean>
	
</beans>
