select

		instance.*,

		p.name as partner_name,
		p.alipay_account,
        p.flower_name,
		p.taobao_nick,
		p.iden_num,
		p.mobile,
		p.email,
		p.business_type,
		p.description as partner_description,
		p.state as partner_state,

		s.name as station_name,
		s.description as station_description,
		s.province,
		s.city,
		s.county,
		s.town,
		s.province_detail,
		s.city_detail,
		s.county_detail,
		s.town_detail,
		s.address,
		s.apply_org,
		s.station_num,
		s.lng,
		s.lat,
		s.village,
		s.village_detail,
		s.covered,
		s.products,
		s.logistics_state,
		s.format,
		s.area_type,
		s.manager_id,
		s.provider_id,
		s.feature,
		s.status,
		s.fixed_type,
		s.category,
		org.name as county_station_name




			,
			life.business_type as lifecycle_business_type,
			life.settled_protocol,
			life.bond,
			life.quit_protocol,
			life.logistics_approve,
			life.current_step,
			life.role_approve,
			life.confirm,
			life.system


		from partner_station_rel instance







		,partner p,station s,cuntao_org org

			, partner_lifecycle_items life


		where instance.partner_id = p.id and instance.station_id=s.id and s.apply_org = org.id
		and instance.is_deleted = 'n' and s.is_deleted='n' and p.is_deleted='n' and org.is_deleted='n'




			and instance.id = life.partner_instance_id



















			and instance.parent_station_id = ?


			and instance.type = ?


			and instance.state = ?


			and instance.is_current = ?








			and life.business_type = ?


			and life.current_step = ?










		order by instance.gmt_modified desc